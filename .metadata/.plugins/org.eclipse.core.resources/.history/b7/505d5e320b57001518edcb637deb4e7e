public class CheeseMaze extends Maze{

	private boolean[][] mazeArray;

	public enum Direction{
		up, down, left, right
	}
	
	public Vertex goal;
	public Vertex start;
	public LinkedList<Vertex> cheese;

	public int mazeWidth;
	public int mazeHeight;
	
	public CheeseMaze(String fileLoc){
		super(fileLoc);
	}
	
}
